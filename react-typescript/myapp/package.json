    "@types/node": "^10.9.4",
    "@types/react": "^16.4.13",
    "@types/react-dom": "^16.0.7",
    "react-app-rewired": "^2.0.3",
    "ts-import-plugin": "^1.5.5",
    "typescript": "^3.0.3"
  },
  "eslintConfig": {
    "extends": "myapp",
    "rules": {
      "no-undef": "1",
      "no-restricted-globals": "1",
      "no-unused-vars": "1"
